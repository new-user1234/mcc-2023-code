def cal(a):
    if a % 2 == 0:
        return a // 2
    else:
        return 3 * a + 1

def repeat(k, lst):
    for _ in range(k):
        lst = [cal(a) for a in lst]
    return lst

given = '633 6067 3340 5573 9445 9180 3610 8235 9350 6939 3982 3044 9636 445 2572 2346 8981 6270 7586 9450 2953 5398 1120 5249 5111 7377 8327 9801 1210 1831 3607 5309 6154 6132 5419 1829 1368 4296 800 6502 3406 729 454 6296 4511 5345 5129 3982 6873 969 6278 8371 8691 9222 8328 8336 2026 5536 2032 337 3962 1158 2978 9556 8432 1790 1131 5006 6760 7564 7810 539 4996 261 1191 5569 8380 2384 2310 2308 7572 8853 571 7524 6608 7777 6429 1040 9859 7096 4339 771 8024 5088 4815 3956 1710 1572 3095 6731 8296 1146 3697 3892 765 3834 4751 5577 6832 6106 2634 9370 5576 3116 2374 3462 7623 4531 3122 380 1566 1757 1722 8590 4525 2494 7164 3011 9148 3867 2815 253 3625 8414 4982 9689 5300 4598 3803 9840 7589 6953 8512 5470 8408 323 6975 6698 2532 9906 3830 131 1196 9672 3841 1806 4740 3485 6722 5484 7804 5641 4161 3605 3313 1557 3158 3559 8247 8796 6830 6478 6931 8913 8569 7677 5150 1430 2966 9029 2288 8287 293 8420 6680 1036 3447 467 5397 9738 1318 4833 7774 3804 8686 4654 1239 6933 4075 1077 8351 6362 2728 3812 9217 2036 7193 9398 2662 1777 9333 7944 74 9732 7889 7177 3811 6936 7097 2856 7195 8098 567 158 2404 3709 53 4568 4321 734 9154 3026 8949 1033 5811 9840 3650 9953 5210 147 7000 9149 1680 8882 9315 816 7481 884 633 730 7777 4228 574 1666 5638 7190 1272 2697 7967 6049 1305 9540 422 4157 8583 7525 4562 2400 4129 911 9392 6506 7909 6470 6882 797 320 418 1466 4296 6682 2370 6609 6894 6502 745 6031 9982 9814 2929 1600 5686 5540 7034 7371 3378 198 3694 4709 5990 2419 3107 8265 2778 983 8106 8107 3089 3430 7078 9251 6890 7413 6050 8360 585 3906 1583 9238 356 1207 9205 8244 5899 8846 1837 905 3562 7312 1566 7699 4658 1927 7147 5560 7368 1510 3168 9922 3718 8156 2846 5158 8852 2842 7844 7267 8699 1753 1524 3662 1562 9614 9267 1463 6665 8719 7316 1429 4975 5906 40 7600 9957 7555 2317 7996 6933 2065 5294 5105 8561 5504 8193 3266 2028 5967 528 7890 2980 800 8928 210 2990 1071 9385 512 6915 8891 7295 1754 9144 8599 1086 2132 3720 6077 3281 3613 4174 1915 4335 3827 2102 3359 1165 8737 2131